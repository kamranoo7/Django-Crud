{
    "version": 2,
    "builds": [
      {
        "src": "crudapp/wasgi.py", 
        "use": "@vercel/python" ,
        "config":{"maxLambdaSize":"15mb","runtime":"python3.9"}
      },
      {
        "src": "build_file.sh", 
        "use": "@vercel/static-build" 
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)", 
        "dest": "/static/$1" 
      },
      {
        "src": "/(.*)",
        "dest":"crudapp/wasgi.py"
      }
    ]
  }
  